{
    "name": "laravel-notification-channels/facebook",
    "description": "Facebook Notifications Channel for Laravel",
    "homepage": "https://github.com/laravel-notification-channels/facebook",
    "license": "MIT",
    "authors": [
        {
            "name": "Irfaq Syed",
            "email": "github@lukonet.net",
            "homepage": "https://lukonet.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "illuminate/notifications": "5.5 - 9",
        "illuminate/support": "5.5 - 9"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^7.5 || ^8.5.21 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Facebook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Facebook\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Facebook\\FacebookServiceProvider"
            ]
        }
    }
}
