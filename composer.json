{
    "name": "laravel-notification-channels/facebook",
    "description": "Facebook Notifications Channel for Laravel 5.3",
    "homepage": "https://github.com/laravel-notification-channels/facebook",
    "license": "MIT",
    "authors": [
        {
            "name": "Syed Irfaq R.",
            "email": "syed@lukonet.com",
            "homepage": "https://lukonet.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/notifications": "5.3.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Facebook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Facebook\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
